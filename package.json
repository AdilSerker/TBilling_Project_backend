{
  "name": "node-elastic",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/app",
    "watch": "./node_modules/.bin/nodemon ./dist/app",
    "build": "./node_modules/.bin/tsc",
    "build-dev": "./node_modules/.bin/tsc --watch",
    "migrate": "node ./dist/bin/dbConfig > ormconfig.json && node ./node_modules/typeorm/cli migration:run",
    "migrate-undo": "node ./dist/bin/dbConfig > ormconfig.json && node ./node_modules/typeorm/cli migration:revert",
    "create-migration": "node ./dist/bin/dbConfig > ormconfig.json && node ./node_modules/typeorm/cli migration:create -n",
    "load-fixtures": "node ./dist/bin/loadFixtures/loader"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.18.3",
    "class-validator": "^0.11.0",
    "connect-redis": "^4.0.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "elasticsearch": "^15.4.1",
    "express": "^4.16.4",
    "express-session": "^1.17.0",
    "intel": "^1.2.0",
    "lodash": "^4.17.12",
    "morgan": "^1.9.1",
    "pg": "^7.9.0",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.7.7",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.16",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.17.0",
    "@types/elasticsearch": "^5.0.32",
    "@types/express": "^4.16.1",
    "@types/express-session": "^1.15.16",
    "@types/lodash": "^4.14.123",
    "@types/morgan": "^1.7.35",
    "@types/redis": "^2.8.16",
    "@types/uuid": "^3.4.4",
    "nodemon": "^1.18.11",
    "typescript": "^3.3.4000"
  }
}
